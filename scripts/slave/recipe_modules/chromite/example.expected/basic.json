[
  {
    "cmd": [
      "[DEPOT_TOOLS]/repo",
      "init",
      "-u",
      "https://chromium.googlesource.com/chromiumos/manifest.git",
      "--repo-url",
      "https://chromium.googlesource.com/external/repo.git"
    ],
    "name": "repo init"
  },
  {
    "cmd": [
      "[DEPOT_TOOLS]/repo",
      "sync"
    ],
    "name": "repo sync"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/chromite/bin/cros_sdk",
      "--cache-dir",
      ".cache",
      "--",
      "./setup_board",
      "--board",
      "amd64-generic"
    ],
    "name": "setup board"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/chromite/bin/cros_sdk",
      "--",
      "./build_packages",
      "--board",
      "amd64-generic"
    ],
    "name": "build packages"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/chromite/bin/cros_sdk",
      "var1=value",
      "--",
      "echo",
      "hello"
    ],
    "name": "cros_sdk"
  },
  {
    "allow_subannotations": true,
    "cmd": [
      "[SLAVE_BUILD]/chromite/bin/cbuildbot",
      "--clobber",
