[
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/bot_update.py",
      "--master",
      "tryserver.v8",
      "--builder",
      "v8_linux64_rel",
      "--slave",
      "TestSlavename",
      "--spec",
      "cache_dir = None\nsolutions = [{'custom_vars': {'chromium_trunk': 'svn://svn-mirror.golo.chromium.org/chrome/trunk'}, 'deps_file': 'DEPS', 'managed': True, 'name': 'v8', 'url': 'https://chromium.googlesource.com/v8/v8'}]",
      "--root",
      "v8",
      "--revision_mapping_file",
      "{\"v8\": \"got_revision\"}",
      "--patch_url",
      "svn://svn-mirror.golo.chromium.org/patch",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "v8@12345",
      "--no_shallow"
    ],
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"v8\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision\": \"f2cbea7e89a45f46819777bb78a1f5bd0241f64c\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision_cp\": \"refs/heads/master@{#50110}\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"v8\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@\"f2cbea7e89a45f46819777bb78a1f5bd0241f64c\"@@@",
      "@@@SET_BUILD_PROPERTY@got_revision_cp@\"refs/heads/master@{#50110}\"@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "runhooks"
    ],
    "env": {
      "GYP_DEFINES": "dcheck_always_on=1 fastbuild=1 gomadir='[BUILD]/goma' target_arch=x64 use_goma=1 v8_target_arch=x64"
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/cleanup_temp.py"
    ],
    "name": "cleanup_temp"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/compile.py",
      "--target",
      "Release",
      "--src-dir",
      "[SLAVE_BUILD]/v8",
      "--build-tool",
      "make",
      "--compiler",
      "goma",
      "--goma-dir",
      "[BUILD]/goma",
      "--"
    ],
    "name": "compile"
  },
  {
    "cmd": [
      "bash",
      "[SLAVE_BUILD]/v8/tools/check-static-initializers.sh",
      "out/Release/d8"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "name": "Static-Initializers"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[v8]/resources/v8testing.py",
      "--target",
      "Release",
      "--arch",
      "x64",
      "--testname",
      "unittests",
      "--no-variants",
      "--flaky-tests=skip",
      "--dcheck-always-on",
      "--json-test-results",
      "/path/to/tmp/json"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "name": "Unittests"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[v8]/resources/v8testing.py",
      "--target",
      "Release",
      "--arch",
      "x64",
      "--testname",
      "default",
      "--flaky-tests=skip",
      "--dcheck-always-on",
      "--json-test-results",
      "/path/to/tmp/json"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "name": "Check"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[v8]/resources/v8testing.py",
      "--target",
      "Release",
      "--arch",
      "x64",
      "--testname",
      "optimize_for_size",
      "--no-variants",
      "--shell_flags=\"--optimize-for-size\"",
      "--flaky-tests=skip",
      "--dcheck-always-on",
      "--json-test-results",
      "/path/to/tmp/json"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "name": "OptimizeForSize"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[v8]/resources/v8testing.py",
      "--target",
      "Release",
      "--arch",
      "x64",
      "--testname",
      "webkit",
      "--flaky-tests=skip",
      "--dcheck-always-on",
      "--json-test-results",
      "/path/to/tmp/json"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "name": "Webkit"
  }
]