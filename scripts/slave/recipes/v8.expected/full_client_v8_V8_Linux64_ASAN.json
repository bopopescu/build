[
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/bot_update.py",
      "--master",
      "client.v8",
      "--builder",
      "V8 Linux64 ASAN",
      "--slave",
      "TestSlavename",
      "--spec",
      "cache_dir = None\nsolutions = [{'custom_deps': {'v8/tools/clang/scripts': 'svn://svn-mirror.golo.chromium.org/chrome/trunk/src/tools/clang/scripts'}, 'custom_vars': {'chromium_trunk': 'svn://svn-mirror.golo.chromium.org/chrome/trunk'}, 'deps_file': 'DEPS', 'managed': True, 'name': 'v8', 'url': 'https://chromium.googlesource.com/external/v8'}]",
      "--revision_mapping_file",
      "{\"v8\": \"got_revision\"}",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "v8@bleeding_edge:20123",
      "--no_shallow"
    ],
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"v8\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision\": 50110, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision_cp\": \"refs/heads/master@{#50110}\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision_git\": \"f2cbea7e89a45f46819777bb78a1f5bd0241f64c\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"v8\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@50110@@@",
      "@@@SET_BUILD_PROPERTY@got_revision_cp@\"refs/heads/master@{#50110}\"@@@",
      "@@@SET_BUILD_PROPERTY@got_revision_git@\"f2cbea7e89a45f46819777bb78a1f5bd0241f64c\"@@@"
    ]
  },
  {
    "cmd": [
      "[SLAVE_BUILD]/v8/tools/clang/scripts/update.sh"
    ],
    "env": {
      "LLVM_URL": "svn://svn-mirror.golo.chromium.org/llvm-project"
    },
    "name": "update clang"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "runhooks"
    ],
    "env": {
      "CC": "[SLAVE_BUILD]/v8/third_party/llvm-build/Release+Asserts/bin/clang",
      "CXX": "[SLAVE_BUILD]/v8/third_party/llvm-build/Release+Asserts/bin/clang++",
      "CXX_host": "[SLAVE_BUILD]/v8/third_party/llvm-build/Release+Asserts/bin/clang++",
      "GYP_DEFINES": "asan=1 clang=1 lsan=1 target_arch=x64 use_allocator=none v8_target_arch=x64",
      "LINK": "[SLAVE_BUILD]/v8/third_party/llvm-build/Release+Asserts/bin/clang++"
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/cleanup_temp.py"
    ],
    "name": "cleanup_temp"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/compile.py",
      "--target",
      "Release",
      "--src-dir",
      "[SLAVE_BUILD]/v8",
      "--build-tool",
      "make",
      "--compiler",
      "clang",
      "--"
    ],
    "name": "compile"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/v8/v8testing.py",
      "--target",
      "Release",
      "--arch",
      "x64",
      "--testname",
      "unittests",
      "--no-variants",
      "--asan",
      "--json-test-results",
      "/path/to/tmp/json"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "env": {
      "ASAN_OPTIONS": "external_symbolizer_path=[SLAVE_BUILD]/v8/third_party/llvm-build/Release+Asserts/bin/llvm-symbolizer"
    },
    "name": "Unittests"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/v8/v8testing.py",
      "--target",
      "Release",
      "--arch",
      "x64",
      "--testname",
      "default",
      "--flaky-tests",
      "skip",
      "--asan",
      "--json-test-results",
      "/path/to/tmp/json"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "env": {
      "ASAN_OPTIONS": "external_symbolizer_path=[SLAVE_BUILD]/v8/third_party/llvm-build/Release+Asserts/bin/llvm-symbolizer"
    },
    "name": "Check"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/v8/v8testing.py",
      "--target",
      "Release",
      "--arch",
      "x64",
      "--testname",
      "default",
      "--flaky-tests",
      "run",
      "--asan",
      "--json-test-results",
      "/path/to/tmp/json"
    ],
    "cwd": "[SLAVE_BUILD]/v8",
    "env": {
      "ASAN_OPTIONS": "external_symbolizer_path=[SLAVE_BUILD]/v8/third_party/llvm-build/Release+Asserts/bin/llvm-symbolizer"
    },
    "name": "Check - flaky"
  }
]