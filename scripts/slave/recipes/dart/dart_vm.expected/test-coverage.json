[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE[build]/bot_update.py",
      "--master",
      "client.dart",
      "--builder",
      "test-coverage-be",
      "--slave",
      "TestSlavename",
      "--spec",
      "cache_dir = None\nsolutions = [{'deps_file': 'DEPS', 'managed': False, 'name': 'sdk', 'url': 'https://chromium.googlesource.com/external/github.com/dart-lang/sdk.git'}]",
      "--root",
      "sdk",
      "--revision_mapping_file",
      "{}",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "sdk@HEAD",
      "--force",
      "--output_manifest"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"sdk\": \"HEAD\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"sdk\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"sdk\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/sdk/tools/clean_output_directory.py"
    ],
    "cwd": "[SLAVE_BUILD]/sdk",
    "name": "clobber"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "runhooks"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/sdk/tools/task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[SLAVE_BUILD]/sdk",
    "name": "taskkill before building"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/sdk/tools/build.py",
      "-mrelease",
      "--arch=x64",
      "runtime"
    ],
    "cwd": "[SLAVE_BUILD]/sdk",
    "env": {
      "CC": "third_party/clang/linux/bin/clang",
      "CXX": "third_party/clang/linux/bin/clang++"
    },
    "name": "build dart"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/sdk/tools/test.py",
      "-mrelease",
      "--arch=x64",
      "--progress=line",
      "--report",
      "--time",
      "--failure-summary",
      "--write-debug-log",
      "--write-test-outcome-log",
      "--copy-coredumps"
    ],
    "cwd": "[SLAVE_BUILD]/sdk",
    "name": "test vm"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/sdk/tools/test.py",
      "-mrelease",
      "--arch=x64",
      "--progress=line",
      "--report",
      "--time",
      "--failure-summary",
      "--write-debug-log",
      "--write-test-outcome-log",
      "--copy-coredumps",
      "--checked"
    ],
    "cwd": "[SLAVE_BUILD]/sdk",
    "name": "test vm (2)"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/sdk/tools/task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[SLAVE_BUILD]/sdk",
    "name": "taskkill after testing"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]