[
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/bot_update.py",
      "--master",
      "client.dart.FYI",
      "--builder",
      "TestBuilder",
      "--slave",
      "TestSlavename",
      "--spec",
      "cache_dir = None\nsolutions = [{'deps_file': 'DEPS', 'managed': False, 'name': 'dart', 'url': 'https://chromium.googlesource.com/external/github.com/dart-lang/sdk.git'}]",
      "--root",
      "dart",
      "--revision_mapping_file",
      "{}",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "dart@HEAD",
      "--force",
      "--output_manifest"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"dart\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"dart\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "runhooks"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/dart/sdk/tools/build.py",
      "-mrelease",
      "runtime"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "build dart"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[SLAVE_BUILD]/dart/sdk/tools/test.py",
      "-mrelease",
      "-cnone",
      "-rvm"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "test vm"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]