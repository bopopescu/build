[
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/bot_update.py",
      "--master",
      "fake_mastername",
      "--builder",
      "fake_buildername",
      "--slave",
      "TestSlavename",
      "--spec",
      "cache_dir = None\nsolutions = [{'deps_file': 'DEPS', 'managed': True, 'name': 'trunk', 'url': 'https://chromium.googlesource.com/external/gyp.git'}]",
      "--root",
      "trunk",
      "--revision_mapping_file",
      "{\"trunk\": \"got_revision\"}",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "trunk@HEAD",
      "--force"
    ],
    "cwd": "[SLAVE_BUILD]",
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
