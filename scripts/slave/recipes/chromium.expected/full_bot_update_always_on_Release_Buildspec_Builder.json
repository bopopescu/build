      "Release",
      "--paths",
      "{\"build\": \"[BUILD]\", \"checkout\": \"[SLAVE_BUILD]/src\"}",
      "--properties",
      "{\"buildername\": \"Release Buildspec Builder\", \"buildnumber\": 571, \"mastername\": \"bot_update.always_on\", \"slavename\": \"TestSlavename\", \"target_platform\": \"linux\"}"
    ],
