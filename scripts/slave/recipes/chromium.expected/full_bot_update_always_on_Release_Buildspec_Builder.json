[
  {
    "cmd": [
      "python",
      "-u",
      "[BUILD]/scripts/slave/bot_update.py",
      "--master",
      "bot_update.always_on",
      "--builder",
      "Release Buildspec Builder",
      "--slave",
      "TestSlavename",
      "--spec",
      "cache_dir = None\nsolutions = [{'custom_vars': {'svn_url': 'svn://svn-mirror.golo.chromium.org'}, 'deps_file': 'DEPS', 'managed': True, 'name': 'chrome_from_buildspec', 'url': 'svn://svn-mirror.golo.chromium.org/chrome-internal/trunk/tools/buildspec/releases/1.2.3.4'}]",
      "--root",
      "chrome_from_buildspec",
      "--revision_mapping_file",
      "{\"chrome_from_buildspec\": \"got_revision\"}",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "chrome_from_buildspec@HEAD"
    ],
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
